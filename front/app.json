{
  "expo": {
    "name": "second-hand",
    "slug": "second-hand",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you use them in your posts."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maazjes.secondhand"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.maazjes.secondhand"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "47.0.0",
    "extra": {
      "eas": {
        "projectId": "50f614ef-880d-4ebb-8b68-cfc9a7a5cb0d"
      }
    }
  },
  "name": "second-hand"
}
